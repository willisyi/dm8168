DSP_CODE_ADDR              = DSS_M3_BSS_ADDR            + DSS_M3_BSS_SIZE;
DSP_DATA_ADDR              = DSP_CODE_ADDR              + DSP_CODE_SIZE;
/* second 512MB */
/* Tiler Buffers in the bottom 512MB */
