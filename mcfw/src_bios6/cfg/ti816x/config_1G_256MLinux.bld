SR2_FRAME_BUFFER_ADDR      = TILER_ADDR                 + TILER_SIZE;
VIDEO_M3_EXCEPTION_CTX_ADDR = SR2_FRAME_BUFFER_ADDR     + SR2_FRAME_BUFFER_SIZE;
