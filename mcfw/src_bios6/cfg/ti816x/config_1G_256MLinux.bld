REMOTE_DEBUG_ADDR          = NOTIFY_SHARED_ADDR         + NOTIFY_SHARED_SIZE;
SR0_ADDR                   =  REMOTE_DEBUG_ADDR         + REMOTE_DEBUG_SIZE;

